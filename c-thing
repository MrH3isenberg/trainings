#include <stdio.h>
#include <string.h>

char tracks[][80] =
{
"I love you",
"Goodbye my lover",
"New York fallen",
};

void find_track(char search_for[])
{
    for(int i = 0; i < 3; i++)
    {
        if(strstr(tracks[i], search_for))
            printf("Song number %i: '%s' \n", i, tracks[i]);

    }
}

int main()
{
    char search_for[80];
    printf("Search: ");
    fgets(search_for, 80, stdin);
    search_for[strlen(search_for) - 1] = '\0';
    find_track(search_for);
    return 0;
    system("pause");
}
